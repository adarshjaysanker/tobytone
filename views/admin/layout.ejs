   <script>
$(document).ready(function() {
  // Add active class to the clicked section and its parent
  $('.menu-link').click(function() {
    $('.menu-item').removeClass('active');
    $(this).parent().addClass('active');
    localStorage.setItem('activeMenuItem', $(this).attr('href'));
  });

  // Add active class to parent when a submenu item is clicked
  $('.submenu a').click(function() {
    $('.menu-item').removeClass('active');
    $(this).closest('.menu-item').addClass('active');
    localStorage.setItem('activeMenuItem', $(this).closest('.menu-item').find('.menu-link').attr('href'));
  });

  // Retrieve active menu item from local storage on page load
  var activeMenuItem = localStorage.getItem('activeMenuItem');
  if (activeMenuItem) {
    $('.menu-item').removeClass('active');
    $('a[href="' + activeMenuItem + '"]').parent().addClass('active');
  }
});


   </script>
