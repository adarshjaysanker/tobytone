        <section class="content-main">
          <form action="/admin/update" method="post" enctype="multipart/form-data">
            <div class="row">
                <div class="col-6">
                    <div class="content-header">
                        <h2 class="content-title">Edit Product</h2>
                    </div>
                </div>
            </div>
         
            <div class="row">
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-3">
                                    <h6>1. General info</h6>
                                </div>
                                <div class="col-md-9">
                                    <div class="mb-4">
                                        <label class="form-label">Product title</label>
                                        <input type="text" placeholder="Type here" name="title" value="<%=item.title%>"   class="form-control">
                                    </div>
                                    <div class="mb-4">
                                        <label class="form-label">Description</label>
                                        <textarea name="description" placeholder="Type here" value="<%=item.description%>" class="form-control" rows="4"><%=item.description%></textarea>
                                    </div>
                                   
                                </div> <!-- col.// -->
                            </div> <!-- row.// -->
                            <hr class="mb-4 mt-0">
                            <div class="row">
                                <div class="col-md-3">
                                    <h6>2. Pricing</h6>
                                </div>
                                <div class="col-md-9">
                                    <div class="mb-4">
                                        <label class="form-label">Cost in USD</label>
                                        <input name="cost" type="text" placeholder="$00.0" value="<%=item.cost%>" class="form-control">
                                    </div>
                                </div> <!-- col.// -->
                            </div> <!-- row.// -->
                            <hr class="mb-4 mt-0">
                            <div class="row">
                                <div class="col-md-3">
                                    <h6>3. Category</h6>
                                </div>
                                 <div class="col-md-9">
                                    <div class="mb-4">
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="True wireless airpodes" type="radio" <%= item.mycategory==="True wireless airdopes" ? 'checked' : '' %>>
                                            <span class="form-check-label"> True wireless airpodes </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="Wireless earphones"  type="radio" <%= item.mycategory==="Wireless earphones" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Wireless earphones </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="wireless headphones"  type="radio" <%= item.mycategory==="Wireless headphones" ? 'checked' : '' %>>
                                            <span class="form-check-label"> wireless headphones </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="Wireless speakers"  type="radio" <%= item.mycategory==="Wireless speakers" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Wireless speakers </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="Soundbars"  type="radio" <%= item.mycategory==="Soundbars" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Soundbars </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="Home Audio" type="radio" <%= item.mycategory==="Home Audio" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Home Audio </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="Party speakers" type="radio" <%= item.mycategory==="Party Speakers" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Party speakers </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="Gaming headphones" type="radio" <%= item.mycategory==="Gaming headphones" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Gaming headphones </span>
                                        </label>
                                        <label class="mb-2 form-check form-check-inline" style="width: 45%;">
                                            <input class="form-check-input" name="mycategory" value="others"  type="radio" <%= item.mycategory==="others" ? 'checked' : '' %>>
                                            <span class="form-check-label"> Others </span>
                                        </label>
                                    </div>
                                </div> 

                                
                            </div> <!-- row.// -->
                            <hr class="mb-4 mt-0">
                            <div class="row">
                                <div class="col-md-3">
                                    <h6>4. Media</h6>
                                </div>
                                <div class="col-md-9">
                                    <div class="mb-4">
                                        <label class="form-label">Images</label>
