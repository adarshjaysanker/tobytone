    <main class="main-wrap">
       
        
     
        <section class="content-main">
           
            <div class="card mb-4">
                
                <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" />
                                        </div>
                                    </th>
                                    <th>#ID</th>
                                    <th>Name</th>
                                    <th>E-mail</th>
                                    <th>Mobile number</th>
                                    <th>Register Date</th>
                                    <th class="text-end">Action</th>
                                </tr>
                            </thead>
                            <tbody>


                                <% users.forEach(function(user) { %>
                                    
                                  
                                <tr>

                                    <td>
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" value="" />
                                        </div>
                                    </td>
                                    <td><%= user._id %></td>
                                    <td><b><%= user.name%></b></td>
                                    <td><%= user.email%></td>
                                    <td>
                                        <%= user.PhoneNumber %>
                                    </td>
                                    <td></td>


                                    <% if (user.isActive) { %>
                                        <td class="col-lg-2 col-sm-2 col-4 col-action text-end"  i>

                                            <a  class="btn btn-success font-sm rounded block-button" data-user-id="<%= user._id %>">
                                                <i class="material-icons "></i> Block
                                            </a>
                                        
                                           
                                     
                                           <!-- dropdown //end -->
                                        </td>
                                      <% } else { %>
                                        <td class="col-lg-2 col-sm-2 col-4 col-action text-end">
                                        
                                            <a  class="btn btn-danger font-sm rounded block-button"  data-user-id="<%= user._id %>">
                                                <i class="material-icons " ></i> Unblock
                                            </a>
                                     
                                           <!-- dropdown //end -->
                                        </td>
                                      <% } %>
                                      

                                    
                                </tr>
                               
                                <% }); %>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive//end -->
                </div>
                <!-- card-body end// -->
            </div>
            <script>
                $(document).ready(function(){
                    $('.block-button').click(function(){
                        var userId=$(this).data('user-id')
                        var button =
                        console.log(userId,"]]]]]]]]")
                        updateUserStatus(userId, $(this))
                    })
                    

                    function updateUserStatus(userId,button){
                        $.ajax({
                            url:'/admin/status-update',
                            method:"POST",
                            data:{
                                userId
                            },
                            success:function (response){
                                console.log(response)
                                button.toggleClass('btn-success btn-danger')
                                button.text(response.active?'block':'unblock');

                                if(!response.active){
                                    $(`#blocked-message`).show();
                                }
                                

                            },
                            error:function(error){
                                console.error(error)
                            }
                        })
                    }
                })
            </script>
